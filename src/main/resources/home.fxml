      <HBox prefHeight="598.0" prefWidth="872.0">
         <children>
            <TableView fx:id="itemTableView" prefHeight="375.0" prefWidth="365.0">
              <columns>
                <TableColumn fx:id="idColumn" prefWidth="39.0" text="id" />
                <TableColumn fx:id="itemNameColumn" prefWidth="203.0" text="item" />
                  <TableColumn fx:id="currentPriceColumn" prefWidth="122.0" text="current price" />
              </columns>
            </TableView>
            <GridPane prefHeight="559.0" prefWidth="293.0">
              <columnConstraints>
